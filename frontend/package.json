    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "daisyui": "^4.12.23",
        "eslint": "^9",
        "eslint-config-next": "15.1.3",
        "husky": "^8.0.0",
        "postcss": "^8",
        "prettier": "3.4.2",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
